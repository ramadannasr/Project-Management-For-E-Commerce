@model IEnumerable<eCommerce.Models.ProjectModule>

@if (Model != null) { 
<table class="table">
    <tr class="mytr">
        <th>
            @Html.DisplayNameFor(model => model.Project.ProjectTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NoOfHoursPerDay)
        </th>
        <th>
           MD Name
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Project.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProjectStatus.Status)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Project.ProjectTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Price)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EndDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NoOfHoursPerDay)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Employee.FirstName)
            @Html.DisplayFor(modelItem => item.Employee.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Project.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProjectStatus.Status)
        </td>
        <td>
          
        </td>
    </tr>
}

</table>
}else{

@Html.Action("emptyMessage","home");
}